{
  "name": "ts-app-starter",
  "version": "1.0.0",
  "description": "TypeScript web app starter template",
  "main": "index.js",
  "scripts": {
    "relink": "npm link ts-wc-starter",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "source": "source-map-explorer public/scripts/my-app.min.js",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:prettier": "prettier 'src/**/*.ts' --list-different || ( echo '↑↑↑  these files are not well formatted  ↑↑↑' && exit 1 )",
    "format:eslint": "eslint 'src/**/*.ts' --fix",
    "format:prettier": "prettier 'src/**/*.ts' --write",
    "generate:duck": "hygen duck create",
    "generate:view": "hygen view create"
  },
  "author": "Daniel J. Lauk <daniel.lauk@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "browser-sync": "^2.26.7",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "connect-logger": "0.0.1",
    "eslint": "^6.4.0",
    "eslint-plugin-jest": "^23.8.0",
    "husky": "^3.0.5",
    "hygen": "^5.0.1",
    "jest": "^25.1.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rollup": "^1.20.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-minify-html-literals": "^1.2.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "source-map-explorer": "^2.0.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@captaincodeman/redux-connect-element": "^2.0.0-beta.1",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-first-routing": "^0.3.0",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "reselect-tools": "0.0.7",
    "tslib": "^1.10.0",
    "universal-router": "^8.2.1"
  }
}
